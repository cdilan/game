box-shadow: 0em 0em 2em .2em rgba(0, 0, 0, 0.25); /* Box com sombra no mozilla firefox */
padding: .5em 1em;
}

.blocks .metabar-pad {
	display: none;
}

.blocks .post-title {
	width: 100%;
//	min-height: 30px;
}

.continue_reading_link {
